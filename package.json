{
  "name": "ai-sketch-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cf": "pnpm clean && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out cache && echo 'Cleaned build directories'",
    "upload:r2": "node scripts/upload-to-r2.js",
    "clean:static": "node scripts/clean-uploaded-assets.js",
    "test:r2": "node scripts/test-r2-setup.js",
    "setup:r2": "node scripts/setup-r2-buckets.js",
    "deploy:full": "pnpm upload:r2 && pnpm build:cf",
    "deploy:clean": "pnpm upload:r2 && pnpm clean:static --force && pnpm build:cf",
    "deploy:preview": "pnpm build:cf && echo 'Ready for Cloudflare Pages deployment'",
    "git:discard": "git reset --hard HEAD && git clean -fd",
    "git:commit": "sh -c 'COMMIT_MSG=\"$1\"; git add . && git commit -m \"$COMMIT_MSG\" && ts-node scripts/update-technical-changelog.ts --contentFile scripts/.tmp_changelog_content.md' --",
    "git:commit:push": "sh -c 'COMMIT_MSG=\"$1\"; git add . && git commit -m \"$COMMIT_MSG\" && ts-node scripts/update-technical-changelog.ts --contentFile scripts/.tmp_changelog_content.md --perform-amend && git push' --",
    "git:push": "git push"
  },
